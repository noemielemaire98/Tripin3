<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:orientation="vertical"
    tools:context=".ui.saved.SavedFragment">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="VOlS SAVED"
        android:id="@+id/bt_flight_saved"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HOTELS SAVED"
        android:id="@+id/bt_hotel_saved"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ACTIVITES SAVED"
        android:id="@+id/bt_activities_saved"/>


</LinearLayout>